<Window x:Class="PetShop.Views.Temp.CountForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PetShop.Views.Temp"
        mc:Ignorable="d"
        Title="CountForm" Height="136" Width="354">
    <Grid Margin="0,0,0,3">
        <Label Content="Укажите количество товара" HorizontalAlignment="Left" Margin="24,15,0,0" VerticalAlignment="Top" Height="36" Width="167"/>
        <Button Content="Подтвердить" IsCancel="True"  HorizontalAlignment="Left" Margin="24,47,0,0" VerticalAlignment="Top" Width="133"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="212,15,0,0" TextWrapping="Wrap" Text="{Binding Count}" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
