<Window x:Class="PetShop.Views.Temp.CountForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PetShop.Views.Temp"
        mc:Ignorable="d"
        Title="CountForm" Height="176.8" Width="354">
    <Grid Margin="0,0,0.2,-1.8">
        <Label Content="Укажите количество товара" HorizontalAlignment="Left" Margin="24,19,0,0" VerticalAlignment="Top" Height="36" Width="188" FontSize="14"/>
        <Button Content="Подтвердить" IsCancel="True"  HorizontalAlignment="Left" Margin="24,80,0,0" VerticalAlignment="Top" Width="151" Height="41"/>
        <TextBox HorizontalAlignment="Left" Height="36" Margin="227,19,0,0" TextWrapping="Wrap" Text="{Binding Count}" VerticalAlignment="Top" Width="110" FontSize="14"/>

    </Grid>
</Window>
